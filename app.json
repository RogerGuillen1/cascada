{
  "expo": {
    "name": "La cascada",
    "slug": "cascada",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icona.png",
    "scheme": "La cascada",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.elrogersit.cascada"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icona.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icona.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9af2b6da-7dd1-4a83-b816-f5d98b8585f5"
      }
    }
  }
}
